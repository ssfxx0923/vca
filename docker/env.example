# V免签 Docker环境配置文件
# 复制此文件为 .env 并修改相应配置

# 应用配置
APP_DEBUG=false
APP_TRACE=false

# 数据库配置
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=vmq
DB_USERNAME=vmq_user
DB_PASSWORD=vmq_password123

# MySQL Root密码
MYSQL_ROOT_PASSWORD=root_password123

# 应用端口配置
APP_PORT=80
MYSQL_PORT=3306
REDIS_PORT=6379
PHPMYADMIN_PORT=8080

# 域名配置（可选）
DOMAIN=localhost

# 安全配置
SECRET_KEY=your_secret_key_here

# 时区配置
TIMEZONE=Asia/Shanghai
